@CHARSET "UTF-8";


html,body{width: 100%;height: 100%;}
.base{width: 100%; height: 100%;}
footer {
	position: fixed;
	bottom: 0px;
	left: 0px;
	width: 100%;
	height: 9%;
	background-color:#fff;
	padding-top: 0.2rem;
	font-size: 0.24rem;
}

.mune_left {
	float: left;
	width: 2.5rem;
	height: 0.7rem;
	text-align: center;
	color: #838383;
}
.mune_right {
	float: right;
	width: 2.5rem;
	height: 0.7rem;
	text-align: center;
	color: #838383;
}
.mune_left a,p {
	font-size: 0.25rem;
	color: #838383;
	margin-top: 0;
}.mune_right a,p {
	font-size: 0.25rem;
	color: #838383;
	margin-top: 0;
}




